package week5;

public class NbyN {

	public static int[][] nbyn(int n) {
		int[][] a = new int[n][n];
		for (int i = 0; i < n; i++) {
			a[i][i]=i;
		}
		return a;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] a = new int[10][10];
		a=nbyn[10];
	}

}
